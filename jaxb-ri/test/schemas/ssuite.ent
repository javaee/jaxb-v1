<?xml version="1.0" encoding="UTF-8"?>

<!-- common things that are used in many ssuite files -->


<!--


   Namespace URIs


-->
<!ENTITY XMLSchemaURI "http://www.w3.org/2001/XMLSchema">
<!ENTITY DatatypesURI "http://www.w3.org/2001/XMLSchema-datatypes">
<!ENTITY rngURI       "http://relaxng.org/ns/structure/1.0">
<!ENTITY JaxbURI      "http://java.sun.com/xml/ns/jaxb">
<!ENTITY XJCURI       "http://java.sun.com/xml/ns/jaxb/xjc">


<!--


   Test Scripts

-->
<!ENTITY % roundtripEquivalenceTest.js SYSTEM
  "scripts/roundtripEquivalenceTest.js">
<!ENTITY roundtripEquivalenceTest
       "%roundtripEquivalenceTest.js;">


<!--
  validating unmarshalling, on-demand validation, marshalling,
  then compare the result with the original
-->
<!ENTITY % validatingRoundtripEquivalenceTest.js SYSTEM
  "scripts/validatingRoundtripEquivalenceTest.js">
<!ENTITY validatingRoundtripEquivalenceTest
       "%validatingRoundtripEquivalenceTest.js;">


<!-- validating unmarshalling, on-demand validation, marshalling  -->
<!ENTITY % validatingRoundtripTest.js SYSTEM
  "scripts/validatingRoundtripTest.js">
<!ENTITY validatingRoundtripTest
       "%validatingRoundtripTest.js;">


<!ENTITY % roundtripTest.js SYSTEM
  "scripts/roundtripTest.js">
<!ENTITY roundtripTest
       "%roundtripTest.js;">


<!ENTITY % unmarshalAndValidateTest.js SYSTEM
  "scripts/unmarshalAndValidateTest.js">
<!ENTITY unmarshalAndValidateTest
       "%unmarshalAndValidateTest.js;">



